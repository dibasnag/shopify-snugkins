{% for block in section.blocks %}
<div class="block-{{block.id}}">
    {% if pages[block.settings.page].handle == page.handle and block.settings.enable %}
        {% if block.type == 'instagram' %}
            {% render 'multi-page-instagram', block: block %}
        {% elsif block.type == 'about' %}
            {% render 'multi-page-about', block: block %}
        {% elsif block.type == 'spotlight-slider' %}
            {% render 'multi-page-spotlight-slider', block: block %}
        {% elsif block.type == 'banner' %}
            {% render 'multi-page-banner', block: block %}
        {% elsif block.type == 'policy' %}
            {% render 'multi-page-policies', block: block %}
        {% elsif block.type == 'feature-product' %}
            {% render 'multi-page-feature-product', block: block %}
        {% elsif block.type == 'custom-block-sub-banner' %}
            {% render 'multi-custom-block-sub-banner', block: block %}
        {% elsif block.type == 'new-arrivals-product' %}
            {% render 'multi-new-arrivals-product', block: block %}
        {% endif %}
    {% endif %}
</div>
{% endfor %}
